{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "new-cap": 0,
    "no-unused-vars": 1,
    "no-undef": 2,
    "no-alert": 0,
    "handle-callback-err": 0,
    "no-underscore-dangle": 0
  }
}
